<% if signed_in? %>
	You signed in!
  <%= link_to "Log out", logout_path, method: "delete" %>
 <% end %>

<%= form_for(:session, url: login_path) do |f|%>

	<%= f.label :email %>
	<%= f.text_field :email %>

	<%= f.label :password %>
	<%= f.password_field :password %>

	<%= f.submit "Sign in" %>

<% end %>
